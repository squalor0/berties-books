<!doctype html>
<html>
  <head>
    <title>Login Audit Log</title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>Login Audit Log</h1>

    <table border="1">
      <tr>
        <th>Username</th>
        <th>Success</th>
        <th>Time</th>
      </tr>

      <% auditRecords.forEach(function(record) { %>
        <tr>
          <td><%= record.username %></td>
          <td><%= record.success ? 'Success' : 'Failed' %></td>
          <td><%= record.time %></td>
        </tr>
      <% }) %>
    </table>

    <p><a href="/">Back to home</a></p>
  </body>
</html>
